{
  "name": "bexistest",
  "version": "0.1.0",
  "description": "automated integration tests for BExIS",
  "scripts": {
    "BEXIS2_BAM_Register": "cross-env NODE_ENV=test mocha --recursive './test/BEXIS2_BAM_register/*.js' --require @babel/register",
    "BEXIS2_Dataset": "cross-env NODE_ENV=test mocha --recursive './test/BEXIS2_Dataset/*.js' --require @babel/register",
    "BEXIS2_BAM_Login": "cross-env NODE_ENV=test mocha --recursive './test/BEXIS2_BAM_login/*.js' --require @babel/register",
    "BEXIS2_BAM_Login:debug": "cross-env NODE_ENV=test mocha --recursive './test/BEXIS2_BAM/*.js' --inspect-brk --require  @babel/register",
    "test:ci": "cross-env NODE_ENV=test mocha --recursive --require @babel/register --reporter mocha-junit-reporter --reporter-options includePending=true"
  },
  "author": "Sirko Schindler",
  "license": "MIT",
  "dependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/node": "^7.8.7",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/register": "^7.9.0",
    "@babel/runtime": "^7.9.6",
    "@types/jquery": "^3.3.38",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "core-js": "^3.6.5",
    "cross-env": "^7.0.2",
    "eslint": "^7.0.0",
    "eslint-loader": "^4.0.2",
    "mocha": "^7.1.2",
    "mocha-eslint": "^6.0.0",
    "mocha-junit-reporter": "^1.23.3",
    "puppeteer": "^3.1.0"
  }
}
